{
  "expo": {
    "name": "iReport Camarines Norte",
    "slug": "ireport_v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logov1_padded_white.png",
    "scheme": "ireportv1",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E88E5",
        "foregroundImage": "./assets/images/logov1_adaptive_white.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ireport_v1",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "jsEngine": "hermes"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logov1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logov1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1E88E5",
          "dark": {
            "image": "./assets/images/logov1-white.png",
            "backgroundColor": "#1E88E5"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logov1_padded_white.png",
          "color": "#1E88E5",
          "defaultChannel": "status-updates"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "updates": {
      "url": "https://u.expo.dev/221989d9-3ee1-47f0-acd0-5896a298d8fb"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "221989d9-3ee1-47f0-acd0-5896a298d8fb"
      }
    }
  }
}
